<%- include header.ejs %>

<%- include sidebar.ejs %>

<div class="items">
    <div class="item__row">
        <%items.forEach(function(item, i) {%>
            <%if (i != 0 && i % 2 == 0) {%>
                </div>
                <div class="item__row">
            <%}%>

            <%- include item.ejs %>

            <%if (i == items.length - 1) {%>&nbsp;<%}%>
        <%})%>
    </div>
</div>

<div class="paginator"></div>

